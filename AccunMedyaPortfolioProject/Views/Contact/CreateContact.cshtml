@model AccunMedyaPortfolioProject.Models.Contact
@{
    ViewBag.Title = "CreateContact";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Yeni İletişim Ekleme Sayfası</h2>

<div class="container">
    <form method="post">

        <label>Numara</label>
        @Html.TextBoxFor(x => x.Phone, new { @class = "form-control" })
        <br />
        <label>Mail</label>
        @Html.TextBoxFor(x => x.Mail, new { @class = "form-control" })
        <br />
        <label>Adres</label>
        @Html.TextBoxFor(x => x.Adress, new { @class = "form-control" })
        <br />

    <button class="btn btn-primary">Yeni iletişim kaydet</button>
    </form>

</div>