@model List<AccunMedyaPortfolioProject.Models.SocialMedia>
<div class="column lg-4 md-5 tab-6 stack-on-550 contact-block">

    <h3 class="text-pretitle">Sosyal Medya</h3>

        <ul class="contact-social">
    @foreach (var item in Model)
    {
            <li><a href="@item.PlatformLink"><img src="@item.ImageUrl" style="width:75px;height:75px" /></a>              
            </li>
    }
        </ul>
</div>

