@model AccunMedyaPortfolioProject.Models.About
@{
    ViewBag.Title = "CreateAbout";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Hakkımda Ekle</h2>
<br />

<div class="container">
    <form method="post">

        <label>Başlık</label>
        @Html.TextBoxFor(x => x.Title, new { @class = "form-control" })
        <br />
        <label>Resim Urlsi</label>
        @Html.TextBoxFor(x => x.ImageUrl, new { @class = "form-control" })
        <br />
        <label>Açıklama</label>
        @Html.TextBoxFor(x => x.Description, new { @class = "form-control" })
        <br />
        <label>CV Link</label>
        @Html.TextBoxFor(x => x.CVLink, new { @class = "form-control" })
        <br />

        <button class="btn btn-info">Ekle</button>
    </form>
</div>
