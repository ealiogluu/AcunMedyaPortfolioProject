@model AccunMedyaPortfolioProject.Models.Expertise
@{
    ViewBag.Title = "CreateExpertise";
    Layout = "~/Views/AdminLayout/Index.cshtml";

}

<h2>Uzmanlık Alanı Ekle</h2>
<br />
<div class="container">
    <form method="post">

        @Html.HiddenFor(x=>x.Id)

        <label style="color:black">Başlık</label>
        @Html.TextBoxFor(x => x.Title, new { @class = "form-control" })
        <br />
      
        <button class="btn btn-info">Kaydet</button>

    </form>
</div>

